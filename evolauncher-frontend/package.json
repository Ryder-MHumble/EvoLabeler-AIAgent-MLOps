{
  "name": "evolauncher-frontend",
  "version": "1.0.0",
  "description": "EvoLabeler - A modern desktop app with exceptional UX",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "vue-i18n": "^9.9.1",
    "element-plus": "^2.5.6",
    "gsap": "^3.12.5",
    "@vueuse/core": "^10.7.2",
    "@vueuse/motion": "^2.1.0"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.33",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "@tailwindcss/typography": "^0.5.10",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.8.27",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.evolabeler.app",
    "productName": "EvoLabeler",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}

