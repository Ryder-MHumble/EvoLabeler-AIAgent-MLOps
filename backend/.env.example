# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Qwen API Configuration (硅基流动)
QWEN_API_KEY=your-qwen-api-key
QWEN_API_BASE_URL=https://api.siliconflow.cn/v1
QWEN_VL_MODEL=Qwen/Qwen3-VL-32B-Instruct
QWEN_TEXT_MODEL=Qwen/Qwen2.5-72B-Instruct

# Remote YOLO Training Configuration
REMOTE_YOLO_PROJECT_PATH=/path/to/yolo/project
REMOTE_TRAINING_SCRIPT=train.py
REMOTE_PREDICT_SCRIPT=predict.py

# Application Configuration
APP_NAME=EvoLabeler-Backend
APP_VERSION=0.1.0
DEBUG=true
LOG_LEVEL=INFO

# File Upload Configuration
MAX_UPLOAD_SIZE_MB=100
ALLOWED_EXTENSIONS=.zip,.tar,.tar.gz

# Job Configuration
JOB_TIMEOUT_SECONDS=3600
MAX_CRAWL_IMAGES=100
PSEUDO_LABEL_CONFIDENCE_THRESHOLD=0.5
